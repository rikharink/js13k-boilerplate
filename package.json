{
  "name": "js13k-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for js13k entries",
  "main": "index.js",
  "repository": "git@github.com:rikharink/js13k-boilerplate.git",
  "author": "Rik Harink <rik@har.ink>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "prestart": "yarn clean",
    "start": "cross-env NODE_ENV=development MINIFY=true rollup -c -w",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production MINIFY=true rollup -c",
    "postbuild": "cross-env ROADROLLER=true node ./bin/postbuild.mjs",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "audit:fix": "npm_config_yes=true npx yarn-audit-fix",
    "check:updates": "npx npm-check-updates"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/html-minifier": "^4.0.2",
    "@types/stats.js": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@webmonetization/types": "^0.0.0",
    "archiver": "^5.3.1",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "ect-bin": "^1.4.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-minifier": "^4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "roadroller": "^2.1.0",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-glsl-optimize": "^1.0.4",
    "rollup-plugin-html2": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "spectorjs": "^0.9.27",
    "terser": "^5.14.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "web-resource-inliner": "^5.0.0"
  },
  "dependencies": {}
}
