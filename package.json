{
  "name": "js13k-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for js13k entries",
  "main": "index.js",
  "repository": "git@github.com:rikharink/js13k-boilerplate.git",
  "author": "Rik Harink <rik@har.ink>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "prestart": "yarn clean",
    "start": "cross-env NODE_ENV=development rollup -c -w",
    "start:full": "cross-env NODE_ENV=development MINIFY=true ROADROLLER=true rollup -c -w",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production MINIFY=true ROADROLLER=true rollup -c",
    "postbuild": "node ./bin/postbuild.mjs",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/html-minifier": "^4.0.2",
    "@types/stats.js": "^0.17.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "archiver": "^5.3.1",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "ect-bin": "^1.4.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-minifier": "^4.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "roadroller": "^2.1.0",
    "rollup": "^2.72.1",
    "rollup-plugin-glsl-optimize": "^1.0.4",
    "rollup-plugin-html2": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "spectorjs": "^0.9.27",
    "terser": "^5.13.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "web-resource-inliner": "^5.0.0"
  },
  "dependencies": {}
}
